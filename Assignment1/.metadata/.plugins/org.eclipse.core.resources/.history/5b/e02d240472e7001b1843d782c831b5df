
public class Aircraft extends PublicTransportation{

	enum Class 
	{
		Helicopter,Airline,Glider,Ballon
	}
	enum Maintenance
	{
		Weekly,Monthly,Yearly
	}
	public Aircraft()
	{
		super();
	}
	public Aircraft(double ticketprice,int numberofsteps)
	{
		super(ticketprice,numberofsteps);
	}
	public Aircraft(Aircraft Aircraft)
	{
		super(Aircraft);
	}
}
